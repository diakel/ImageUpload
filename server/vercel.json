{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/check/(.*)",
            "dest": "./index.js"
        },
        {
            "src": "/api/s3/(.*)",
            "dest": "./index.js"
        }
    ]
}