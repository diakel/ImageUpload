{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/check/nsfw",
            "dest": "/routes/check.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "https://image-upload-frontend-one.vercel.app"
            }
        },
        {
            "src": "/api/s3/signed_url",
            "dest": "/",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}