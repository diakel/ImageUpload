{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/check/(.*)",
            "dest": "/api/check.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        },
        {
            "src": "/api/s3/(.*)",
            "dest": "/api/misc.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
            "headers": {
              "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}